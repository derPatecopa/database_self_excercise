  "main": "server.ts",
  "scripts": {
    "log": "npm run build && node dist/models/book.js",
    "migrate": "db-migrate up --config database.json -e dev",
    "migrate-down": "db-migrate down --config database.json -e test && db-migrate down --config database.json -e dev",
    "migrate-test": "db-migrate up --config database.json -e test",
    "migrate-test-down": "db-migrate down --config database.json -e test",
    "jasmine": "jasmine",
    "start": "nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "test": "export ENV=test npx tsc && npx tsc && db-migrate db:create udacity_test_database && db-migrate --env test up && jasmine && db-migrate db:drop udacity_test_database",
    "tsc": "tsc",
    "build": "npx tsc",
    "prettier": "npx prettier --write src/",
    "lint": "eslint . --ext .ts"
  },
